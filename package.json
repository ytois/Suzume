{
  "name": "Suzume",
  "displayName": "Suzume",
  "description": "mini twitter client",
  "version": "0.0.1",
  "publisher": "ytois",
  "engines": {
    "vscode": "^1.29.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./extension",
  "contributes": {
    "commands": [
      {
        "command": "extension.showTimeline",
        "title": "Suzume: Show Timeline"
      },
      {
        "command": "extension.postTweet",
        "title": "Suzume: Post Tweet"
      }
    ]
  },
  "scripts": {
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "node ./node_modules/vscode/bin/test"
  },
  "dependencies": {
    "moment": "^2.22.2",
    "twitter-lite": "^0.7.0"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.42",
    "@types/node": "^8.10.25",
    "babel-preset-es2015": "^6.24.1",
    "babel-register": "^6.26.0",
    "eslint": "^4.11.0",
    "typescript": "^3.1.4",
    "vscode": "^1.1.25"
  }
}
